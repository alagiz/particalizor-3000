{
  "name": "particalizor-3000",
  "version": "0.2.3",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "dependencies": {
    "ramda": "^0.26.1",
    "styled-components": "^4.4.0"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "tsc && copyfiles -u 1 ./src/assets/* ./lib",
    "eject": "react-scripts eject",
    "test": "react-scripts test --coverage --watchAll=false",
    "test-ci": "CI=true TZ='Europe/Amsterdam' react-scripts test",
    "coverage": "react-scripts test --coverage --watchAll=false",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "lint": "eslint src/**/*.{js,ts,tsx} --fix",
    "stylelint": "stylelint src/**/Styled/**",
    "prepare": "npm run build"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "yarn lint",
    "*.{ts}": "yarn stylelint"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.2",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.8",
    "@types/ramda": "^0.26.5",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/sinon": "^7.0.13",
    "@types/styled-components": "^4.1.19",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "@welldone-software/why-did-you-render": "^3.3.6",
    "copyfiles": "^2.1.1",
    "coveralls": "^3.0.9",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.14.3",
    "jest-canvas-mock": "^2.2.0",
    "lint-staged": "^9.4.2",
    "polished": "^3.4.1",
    "prettier": "^1.18.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.2.0",
    "sinon": "^7.3.2",
    "stylelint": "^11.1.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-no-unsupported-browser-features": "^3.0.2",
    "stylelint-processor-styled-components": "^1.8.0",
    "typescript": "^3.6.3"
  },
  "description": "react library capable of particalizing images",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArtemAlagizov/particalizor-3000.git"
  },
  "keywords": [
    "particle"
  ],
  "author": "artem.alagizov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ArtemAlagizov/particalizor-3000/issues"
  },
  "files": [
    "lib/**/*"
  ]
}
